{
	"name": "back-to-the-past",
	"version": "1.0.0",
	"description": "通过快捷键方式快速返回上次观看的位置 | Quickly return to the last viewed position using keyboard shortcuts",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": ["obsidian-plugin", "obsidian", "back", "history"],
	"author": "HOWILLMAKEIT",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/HOWILLMAKEIT/back-to-the-past"
	},
	"funding": {
